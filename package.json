{
  "name": "webui",
  "version": "0.0.0",
  "private": true,
  "description": "Well abstracted components using latest HTML & CSS capabilities.",
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "workspaces": [
    "packages/vue"
  ],
  "scripts": {
    "docs:build": "vitepress build docs",
    "docs:dev": "vitepress dev docs",
    "docs:preview": "vitepress preview docs",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview",
    "format": "prettier --write docs/.vitepress packages/vue/src",
    "lint": "eslint . --fix",
    "prepare": "husky",
    "type-check": "vue-tsc --build --force",
    "vue": "npm run --workspace=@webui/vue"
  },
  "dependencies": {
    "@fontsource-variable/jura": "^5.1.0",
    "@fontsource-variable/nunito": "^5.1.0",
    "@heroicons/vue": "^2.1.5",
    "@histoire/plugin-vue": "^0.17.17",
    "@vueuse/core": "^11.2.0",
    "histoire": "^0.17.17",
    "tailwindcss": "^3.4.15",
    "vitepress": "^1.5.0",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-vue": "^9.31.0",
    "husky": "^9.1.6",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "vue-tsc": "^2.1.10"
  }
}
