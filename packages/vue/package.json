{
  "name": "webui.dev",
  "version": "0.0.1",
  "description": "Well-abstracted code patterns that leverage the latest capabilities of the web-platform.",
  "keywords": [
    "web",
    "ui",
    "dialog",
    "details",
    "popover",
    "select"
  ],
  "homepage": "https://github.com/qruto/webui#readme",
  "bugs": {
    "url": "https://github.com/qruto/webui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qruto/webui.git"
  },
  "license": "MIT",
  "author": {
    "name": "Slava Razum",
    "email": "razum@qruto.to",
    "url": "https://github.com/qruto"
  },
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/webui.js"
    }
  },
  "main": "dist/webui.js",
  "module": "dist/webui.js",
  "types": "dist/webui.d.ts",
  "bin": {
    "webui": "dist/bin/webui-cli.js"
  },
  "files": [
    "dist",
    "src",
    "package.json",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "dev": "vite",
    "dev:cli": "tsup --watch",
    "build": "run-p type-check build:cli \"build-only {@}\" --",
    "build-only": "vite build",
    "build:cli": "tsup",
    "type-check": "vue-tsc --build --force",
    "test": "vitest --run",
    "test:coverage": "vitest --coverage --run",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@vueuse/core": "^12.7.0",
    "commander": "^13.0.0",
    "dedent": "^1.5.3",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.2",
    "@types/prompts": "^2.4.9",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/browser": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8",
    "@vue/tsconfig": "^0.7.0",
    "jsdom": "^25",
    "memfs": "^4.17.0",
    "msw": "^2.7.0",
    "npm-run-all2": "^7.0.2",
    "playwright": "^1.49.1",
    "start-server-and-test": "^2.0.9",
    "tsup": "^8.3.6",
    "vite": "^5.4",
    "vite-plugin-dts": "^4.4.0",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8",
    "vitest-browser-vue": "^0.0.1",
    "vue-tsc": "^2.2.0"
  },
  "peerDependencies": {
    "vue": "^3.5.12"
  }
}
