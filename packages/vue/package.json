{
  "name": "@webui/vue",
  "version": "0.1.0",
  "description": "Well-abstracted components that leverage the latest capabilities of the web platform.",
  "keywords": [
    "web",
    "ui",
    "dialog",
    "details",
    "popover",
    "select"
  ],
  "homepage": "https://github.com/qruto/webui#readme",
  "bugs": {
    "url": "https://github.com/qruto/webui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qruto/webui.git"
  },
  "license": "MIT",
  "author": {
    "name": "Slava Razum",
    "email": "razum@qruto.to",
    "url": "https://github.com/qruto"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/webui.js",
      "require": "./dist/webui.cjs"
    }
  },
  "bin": {
    "webui": "dist/bin/webui-cli.js"
  },
  "main": "dist/webui.cjs",
  "module": "dist/webui.js",
  "types": "dist/webui.d.ts",
  "files": [
    "dist",
    "src",
    "package.json",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-s type-check \"build-only {@}\" build-cli --",
    "build-only": "vite build",
    "build-cli": "vite build --config vite.cli.config.ts",
    "build-cli-tsup": "tsup",
    "type-check": "vue-tsc --build --force",
    "test:unit": "vitest --run",
    "test:coverage": "vitest --coverage --run",
    "test:unit:watch": "vitest --watch",
    "format": "prettier --write ./src",
    "lint": "eslint . --fix"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^5.1.5",
    "@vitest/eslint-plugin": "1.1.8",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.6.0",
    "@vueuse/core": "^11.1.0",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.3.3",
    "start-server-and-test": "^2.0.8",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-vue-devtools": "^7.6.4",
    "vitest": "^2.1.4",
    "vue-tsc": "^2.1.10"
  },
  "peerDependencies": {
    "vue": "^3.5.12"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "prompts": "^2.4.2",
    "tsup": "^8.3.5",
    "zod": "^3.23.8"
  }
}
